<section class="added-travels">
    @foreach (var destination in Model)
    {
                <div class="added-travel">
                    <img src=@destination.ImageUrl alt="" class="picture-added-travel">
                <h3>From @destination.Origin to @destination.DestinationName</h3>
                    <span>Tickets #@destination.Tickets </span>
                    <span>@destination.Date @destination.Time</span>
                <a href="/Tickets/Create?destinationId=X" class="addTickets">Add Tickets</a>
                <a href="/Tickets/Reserve?destinationId=X" class="bookTrip">Book a Trip</a>
            </div>
    }
</section>